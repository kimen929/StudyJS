<%- include header %>
<% posts.forEach(function (post, index) { %>
  <p><h2><a href="/u/<%= post.name %>/<%= post.time.day %>/<%= post.title %>"><%= post.title %></a></h2><a href="/u/<%= post.name %>"><img src="<%= post.head %>" class="r_head" /></a></p>
  <p class="info">
    Author: <a href="/u/<%= post.name %>"><%= post.name %></a> | 
    Date: <%= post.time.minute %>
    Tags: 
  <% post.tags.forEach(function (tag, index) { %>
    <% if (tag) { %>
      <a class="tag" href="/tags/<%= tag %>"><%= tag %></a>
    <% } %>
  <% }) %>
  <% if (post.reprint_info.reprint_from) { %>
    <br><a href="/u/<%= post.reprint_info.reprint_from.name %>/<%= post.reprint_info.reprint_from.day %>/<%= post.reprint_info.reprint_from.title %>">link</a>
  <% } %>
  </p>
  <p><%- post.post %></p>
  <p class="info">Visits:<%= post.pv %> | Comments:<%= post.comments.length     %></p>
<% }) %>
<%- include paging %>
<%- include footer %>
